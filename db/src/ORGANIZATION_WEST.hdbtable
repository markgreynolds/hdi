column table "ORGANIZATION_WEST" (
    "WEST_ID" INTEGER not null COMMENT 'Organization ID',
    "WEST_NAME"  NVARCHAR(200) COMMENT 'Organization Name',
    "WEST_ADDRESS"  NVARCHAR(200) COMMENT 'Primary Address',
    "WEST_CITY" NVARCHAR(200) COMMENT 'Primary City',
    "WEST_CONTACT" NVARCHAR(200) COMMENT 'Primary Contact Name',
    "WEST_RATING" INTEGER COMMENT 'Overall rating',
    "WEST_SALES" DOUBLE COMMENT 'Total Sales',
    PRIMARY KEY ("WEST_ID")
)
COMMENT 'Customer status information'